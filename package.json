{
  "name": "@donkeyclip/motorcortex-threejs",
  "version": "2.4.2",
  "description": "Threejs library for MotorCortex",
  "main": "dist/motorcortex-threejs.cjs.js",
  "module": "dist/motorcortex-threejs.esm.js",
  "browser": "dist/motorcortex-threejs.umd.js",
  "files": [
    "dist"
  ],
  "author": "Donkeyclip (donkeyclip.com) <opensource@donkeyclip.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/donkeyclip/motorcortex-threejs.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rollup -c",
    "start": "webpack serve --hot --config ./demo/webpack.config.js --mode=development",
    "build:demo": "webpack --mode=production --config ./demo/webpack.config.js",
    "test": "HERE GOES YOUR TEST TASK",
    "test:prod": "npm run lint",
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "prepare": "husky install"
  },
  "keywords": [
    "motorcortex",
    "animation",
    "threejs",
    "3d"
  ],
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{json,md,yml,yaml,css}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@donkeyclip/motorcortex-player": "^2.3.5",
    "three": "^0.134.0"
  },
  "peerDependencies": {
    "@donkeyclip/motorcortex": ">= 7.5.4 < 8"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.16.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.16.0",
    "@babel/plugin-syntax-jsx": "7.16.0",
    "@babel/plugin-transform-react-jsx": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@donkeyclip/motorcortex": "7.5.5",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@size-limit/preset-big-lib": "6.0.4",
    "babel-loader": "8.2.3",
    "browserslist": "4.18.0",
    "caniuse-lite": "1.0.30001280",
    "commitizen": "4.2.4",
    "concurrently": "6.4.0",
    "core-js": "3.19.1",
    "css-loader": "6.5.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.1",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "path-browserify": "1.0.1",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "rollup": "2.60.0",
    "rollup-plugin-cleanup": "3.2.1",
    "rollup-plugin-terser": "7.0.2",
    "semantic-release": "18.0.0",
    "size-limit": "6.0.4",
    "style-loader": "3.3.1",
    "uuid": "8.3.2",
    "webpack": "5.64.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.5.0"
  }
}
