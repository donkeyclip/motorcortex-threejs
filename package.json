{
  "name": "@donkeyclip/motorcortex-threejs",
  "version": "3.2.2",
  "description": "Threejs library for MotorCortex",
  "main": "dist/motorcortex-threejs.cjs.js",
  "module": "dist/motorcortex-threejs.esm.js",
  "browser": "dist/motorcortex-threejs.umd.js",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md",
    "LICENCE"
  ],
  "author": "Donkeyclip (donkeyclip.com) <opensource@donkeyclip.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/donkeyclip/motorcortex-threejs.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint -c .eslintrc src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rollup -c",
    "start": "webpack serve --hot --config ./demo/webpack.config.js --mode=development",
    "build:demo": "webpack --mode=production --config ./demo/webpack.config.js",
    "test": "HERE GOES YOUR TEST TASK",
    "test:prod": "npm run lint",
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "prepare": "husky install"
  },
  "keywords": [
    "motorcortex",
    "animation",
    "threejs",
    "3d"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{json,md,yml,yaml,css}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "three": "^0.148.0"
  },
  "peerDependencies": {
    "@donkeyclip/motorcortex": ">=8 < 10"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@babel/eslint-parser": "7.19.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-syntax-jsx": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.20.7",
    "@babel/preset-env": "7.20.2",
    "@donkeyclip/motorcortex": "9.5.7",
    "@donkeyclip/motorcortex-player": "2.10.8",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "babel-loader": "8.3.0",
    "browserslist": "4.21.4",
    "caniuse-lite": "1.0.30001443",
    "concurrently": "7.6.0",
    "core-js": "3.27.1",
    "css-loader": "6.7.3",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.27.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.2",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-cleanup": "3.2.1",
    "rollup-plugin-terser": "7.0.2",
    "style-loader": "3.3.1",
    "uuid": "8.3.2",
    "webpack": "5.75.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  }
}
